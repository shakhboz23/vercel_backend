{
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/api/swagger-ui/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
}